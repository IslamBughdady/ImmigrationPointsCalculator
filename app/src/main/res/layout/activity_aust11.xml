<?xml version="1.0" encoding="utf-8"?>
<android.widget.RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.mansour.ig.pointscalculator.Aust11">

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:alpha=".09"
        android:scaleType="fitXY"
        app:srcCompat="@drawable/background" />

    <ProgressBar
        android:id="@+id/indicator11"
        style="?android:attr/progressBarStyleHorizontal"
        android:layout_width="match_parent"
        android:layout_height="35dp"
        android:layout_marginTop="1dp"
        android:background="#020c5b"
        android:max="140"
        android:progressBackgroundTint="#ff00" />

    <TextView
        android:id="@+id/score11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="55dp"
        android:alpha=".9"
        android:text="your points are:"
        android:textColor="#ff00"
        android:textSize="20dp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/visa"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="120dp"
        android:alpha=".9"
        android:text="VisaSubclass"
        android:textColor="#020c5b"
        android:textSize="22dp"
        android:textStyle="bold" />

    <RadioGroup
        android:id="@+id/yesorno"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="19dp"
        android:layout_marginTop="210dp"
        android:background="#fd020c5b">

        <RadioButton
            android:id="@+id/indep"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:buttonTint="#ff00"
            android:onClick="pressed_5"
            android:text="Skilled Independent Subclass 189"
            android:textColor="#fff"
            android:textSize="18dp"
            android:textStyle="bold" />

        <RadioButton
            android:id="@+id/nomi"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:buttonTint="#ff00"
            android:onClick="pressed_5"
            android:text="Skilled Nominated Subclass 190"
            android:textColor="#fff"
            android:textSize="18dp"
            android:textStyle="bold" />

        <RadioButton
            android:id="@+id/reg"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:buttonTint="#ff00"
            android:onClick="pressed_5"
            android:text="Skilled Regional(Provisional) Subclass 489"
            android:textColor="#fff"
            android:textSize="18dp"
            android:textStyle="bold" />
    </RadioGroup>

    <TextView
        android:id="@+id/warning"
        android:layout_width="300dp"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="350dp"
        android:alpha=".9"
        android:textColor="#ff00"
        android:textSize="16dp"
        android:textStyle="bold" />

    <Button
        android:id="@+id/prev11"
        android:layout_width="80dp"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_marginLeft="30dp"
        android:layout_marginTop="410dp"
        android:background="#020c5b"
        android:text="previous"
        android:textColor="#ffffff"
        android:textSize="15dp"
        android:textStyle="bold" />
    <com.google.android.gms.ads.AdView
        xmlns:ads="http://schemas.android.com/apk/res-auto"
        android:id="@+id/adView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_alignParentBottom="true"
        ads:adSize="BANNER"
        ads:adUnitId="ca-app-pub-9339624373452507/9973728953">
    </com.google.android.gms.ads.AdView>


</android.widget.RelativeLayout>
